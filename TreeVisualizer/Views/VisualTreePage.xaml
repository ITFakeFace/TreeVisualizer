            x:Name="Main"
            Canvas.Left="0"
            Canvas.Top="0"
            Width="1700"
            Height="1080"
            Panel.ZIndex="5"
            Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="150" />
                <RowDefinition Height="*" />
                <RowDefinition Height="120" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

            <Grid Panel.ZIndex="3">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="200" />
                </Grid.ColumnDefinitions>
                <!--  New NavBar  -->
                <Grid Grid.Row="0" Grid.Column="1">
                    <StackPanel
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Orientation="Horizontal">
                        <toolbar:ToolBarItemUserControl x:Name="ModeCreate" Mode="Create" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeDelete" Mode="Delete" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeTraverse" Mode="Traverse" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeMove" Mode="Move" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeSave" Mode="Export" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeImport" Mode="Import" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeChangeTree" Mode="ChangeTreeType" />
                        <!--
                        <toolbar:ToolBarItemUserControl x:Name="ModeUpdate" Mode="Update" />
                        <toolbar:ToolBarItemUserControl x:Name="ModeSearch" Mode="Search" />
                        -->
                    </StackPanel>
                </Grid>
                <!--  End New Navbar  -->
            </Grid>

            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="200" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="NodeContainerParent"
                    Grid.Column="1"
                    BorderBrush="Black"
                    BorderThickness="2"
                    ClipToBounds="True">
                    <Canvas
                        x:Name="canvas"
                        Canvas.Left="210"
                        Canvas.Top="140"
                        ClipToBounds="False">
